  script.
    async function TaoQuanHe() {
      document.querySelector("#encoded").innerHTML = "Đang xử lý..."
      var form = document.querySelector("#formQH");

      data = {
        LuocDo: form.querySelector("input[name='LuocDo']").value,
        SQLSchema: form.querySelector("textarea[name='SQLSchema']").value,
        type: 'TaoQuanHe'     
      }

      let response = await fetch('./ThemCauHoi', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify(data),
        json: true
      })

      let text = await response.json(); 
      console.log(text)
      //alert(text.message);
      document.querySelector("#encoded").innerHTML = text.message;
    }

  script.
    async function TaoCauHoiVaTestcase() {
      document.querySelector("#encoded").innerHTML = "Đang xử lý..."
      var formCH = document.querySelector("#formCH");
      var formOutput = document.querySelector("#formOutput");
      var TinhTrang = form.querySelector("input[name='TinhTrang']").value;
      if(TinhTrang.checked == true) {
        TinhTrang = 1;
      } else {
        TinhTrang = 0;
      }

      data = {
        TieuDe: formCH.querySelector("input[name='TieuDe']").value,
        NoiDung: formCH.querySelector("textarea[name='NoiDung']").value,
        MucDo: formCH.querySelector("select[name='DoKho']").value,
        SQLQuery: formOutput.querySelector("textarea[name='SQLQuery']").value,
        TinhTrang: TinhTrang,
        type: 'SuaCauHoi'
      }

      let response = await fetch('./', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify(data),
        json: true
      })

      let text = await response.json(); 
      console.log(text)
      //alert(text.message);
      document.querySelector("#encoded").innerHTML = text.message;
    }

  script.
    async function KiemThuTestCase() {
      document.querySelector("#encoded").innerHTML = "Đang xử lý..."
      var formOutput = document.querySelector("#formOutput");

      data = {
        SQLQuery: formOutput.querySelector("textarea[name='SQLQuery']").value,
        type: 'KiemThuTestCase'
      }

      let response = await fetch('./', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify(data),
        json: true
      })

      let text = await response.json(); 
      console.log(text)
      //alert(text.message);
      if(text.statusCode != 200)
        document.querySelector("#encoded").innerHTML = text.message;
      else
        document.querySelector("#encoded").innerHTML = text.result;
      document.querySelector("#encoded").style="margin: auto"
    }
  
  script.
    async function XoaCauHoi() {
      console.log("XoaCauHoi")
      if(!window.confirm("Bạn có chắc chắn muốn xóa câu hỏi này không?")) 
        return;
      

      data = {
        type: 'XoaCauHoi'
      }

      let response = await fetch('./', {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json',
        },
        body: JSON.stringify(data),
        json: true
      })

      let text = await response.json(); 
      console.log(text)
      //alert(text.message);
      window.alert(text.message);
      window.location.href = "/admin/QuanLyCauHoi";
    }